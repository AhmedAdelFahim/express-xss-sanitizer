{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "error",
        "no-param-reassign": "off",
        "comma-dangle": "error",
        "prettier/prettier": ["error"],
        "quotes": "off",
        "strict": "off",
        "prefer-arrow-callback":"off",
        "operator-linebreak": "off"
    }
}
